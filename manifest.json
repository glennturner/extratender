{
	"name": "ExtraTender",
	"version": "0.46",
	"manifest_version": 2,
	"description": "An optimized Tenderapp.com for support techs.",
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	           "128": "icon128.png"
	},
	"background": {
		"scripts": [ "background.js" ]
	},
	"content_scripts": [
		{
			"matches": [ "https://*.tenderapp.com/*", "http://*.tenderapp.com/*" ],
			"js": [ "lib/mousetrap.min.js", "lib/util.js", "lib/tender.js", "lib/extratender.js", "contentscript.js" ],
			"css": [ "css/extratender.css" ]
		}
	],
	"permissions": [
		"tabs",
		"storage",
		"*://*.tenderapp.com/*"
	],
	 "options_page": "fancy-settings/index.html"
}